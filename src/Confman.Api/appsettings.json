{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Confman.Api.Cluster.ConfigStateMachine": "Debug",
      "DotNext": "Debug",
      "DotNext.Net.Cluster.Consensus.Raft.PersistentState": "Debug",
      "DotNext.Net.Cluster.Consensus.Raft.StateMachine": "Debug",
      "DotNext.Net.Cluster.Consensus.Raft.Http.RaftHttpCluster": "Warning"
    }
  },
  "AllowedHosts": "*",
  "partitioning": false,
  "lowerElectionTimeout": 3000,
  "upperElectionTimeout": 6000,
  "requestTimeout": "00:00:05",
  "heartbeatThreshold": 0.3,
  "coldStart": false,
  "ReadBarrier": {
    "Enabled": true,
    "FailureMode": "reject",
    "TimeoutMs": 5000
  },
  "Storage": {
    "DataPath": "./data",
    "LogApplies": true
  },
  "Api": {
    "LogConfigChanges": false,
    "LogDataStoreApplies": false
  },
  "Auth": {
    "ApiKeys": [
      {
        "Key": "confman_dev_abc123",
        "Name": "Development Admin",
        "Roles": ["admin"]
      },
      {
        "Key": "confman_dev_reader",
        "Name": "Development Reader",
        "Roles": ["viewer"]
      }
    ]
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore": "Warning",
        "Confman.Api.Cluster.ConfigStateMachine": "Debug",
        "DotNext": "Debug",
        "DotNext.Net.Cluster.Consensus.Raft.PersistentState": "Debug",
        "DotNext.Net.Cluster.Consensus.Raft.StateMachine": "Debug",
        "DotNext.Net.Cluster.Consensus.Raft.Http.RaftHttpCluster": "Warning"
      }
    }
  }
}
