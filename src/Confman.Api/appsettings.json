{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "protocolVersion": "http2",
  "partitioning": false,
  "lowerElectionTimeout": 1000,
  "upperElectionTimeout": 3000,
  "requestTimeout": "00:00:10",
  "heartbeatThreshold": 0.3,
  "coldStart": false,
  "ReadBarrier": {
    "Enabled": true,
    "FailureMode": "stale",
    "TimeoutMs": 5000
  },
  "Raft": {
    "SnapshotInterval": 300,
    "FlushIntervalMs": 500,
    "BatchMaxSize": 50,
    "BatchMaxWaitMs": 1,
    "BatchMaxBytes": 4194304
  },
  "Storage": {
    "DataPath": "./data",
    "LogApplies": false
  },
  "Api": {
    "LogConfigChanges": false,
    "LogDataStoreApplies": false
  },
  "Audit": {
    "Enabled": false
  },
  "BlobStore": {
    "Enabled": true,
    "InlineThresholdBytes": 65536,
    "ClusterToken": "",
    "MaxBlobSizeBytes": 52428800
  },
  "Auth": {
    "ApiKeys": [
      {
        "Key": "confman_dev_abc123",
        "Name": "Development Admin",
        "Roles": ["admin"]
      },
      {
        "Key": "confman_dev_reader",
        "Name": "Development Reader",
        "Roles": ["viewer"]
      }
    ]
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore": "Warning",
        "System.Net.Http.HttpClient": "Warning",
        "Confman.Api.Storage.Blobs": "Debug",
        "Confman.Api.Services": "Debug",
        "Confman.Api.Controllers.InternalBlobController": "Debug"
      }
    }
  }
}
