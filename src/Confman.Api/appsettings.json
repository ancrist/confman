{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "DotNext": "Debug"
    }
  },
  "AllowedHosts": "*",
  "partitioning": false,
  "lowerElectionTimeout": 150,
  "upperElectionTimeout": 300,
  "requestTimeout": "00:00:05",
  "heartbeatThreshold": 0.5,
  "members": [
    "http://localhost:5000",
    "http://localhost:5001",
    "http://localhost:5002"
  ],
  "Storage": {
    "DataPath": "./data"
  },
  "Auth": {
    "ApiKeys": [
      {
        "Key": "confman_dev_abc123",
        "Name": "Development Admin",
        "Principal": "dev@local",
        "Role": "admin"
      },
      {
        "Key": "confman_dev_reader",
        "Name": "Development Reader",
        "Principal": "reader@local",
        "Role": "reader"
      }
    ]
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore": "Warning",
        "DotNext": "Debug"
      }
    }
  }
}
