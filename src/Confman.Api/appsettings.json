{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "DotNext": "Warning",
      "DotNext.Net.Cluster.Consensus.Raft.Http.RaftHttpCluster": "Error"
    }
  },
  "AllowedHosts": "*",
  "partitioning": false,
  "lowerElectionTimeout": 1000,
  "upperElectionTimeout": 2000,
  "requestTimeout": "00:00:05",
  "heartbeatThreshold": 0.5,
  "coldStart": false,
  "ReadBarrier": {
    "Enabled": true,
    "FailureMode": "reject",
    "TimeoutMs": 5000
  },
  "Storage": {
    "DataPath": "./data"
  },
  "Auth": {
    "ApiKeys": [
      {
        "Key": "confman_dev_abc123",
        "Name": "Development Admin",
        "Roles": ["admin"]
      },
      {
        "Key": "confman_dev_reader",
        "Name": "Development Reader",
        "Roles": ["reader"]
      }
    ]
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore": "Warning",
        "DotNext": "Warning",
        "DotNext.Net.Cluster.Consensus.Raft.Http.RaftHttpCluster": "Error"
      }
    }
  }
}
